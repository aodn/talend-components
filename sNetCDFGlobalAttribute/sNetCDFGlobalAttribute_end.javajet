} finally {
    if (null != ncfile_<%=cid %>) try {
        ncfile_<%=cid %>.close();
    } catch (Exception e) {
        System.out.println("Error: ");
        e.printStackTrace();
    }
}
globalMap.put("<%=cid %>_NB_LINE",nbLine_<%=cid %>);
