 <COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.101"
    STATUS="ALPHA"
    IS_MULTIPLYING_OUTPUTS="true"
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20050320A"
    STARTABLE="false"
    DATA_AUTO_PROPAGATE="false"
  >
    <SIGNATURE></SIGNATURE>
  </HEADER>

  <FAMILIES>
    <FAMILY>Processing/Fields</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MIN_OUTPUT="1" MAX_OUTPUT="1"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>

    <PARAMETER
      NAME="SCHEMA"
      FIELD="SCHEMA_TYPE"
      REQUIRED="true"
      NUM_ROW="10"
    >
    </PARAMETER>	

    <PARAMETER NAME="NORMALIZE_COLUMNS" FIELD="TABLE" REQUIRED="true" NUM_ROW="30" NB_LINES="5" SHOW="true">
        <ITEMS>
            <ITEM NAME="NORMALIZE_COLUMN" FIELD="PREV_COLUMN_LIST"/>
        </ITEMS>
    </PARAMETER>

    <PARAMETER
      NAME="ITEMSEPARATOR"
      FIELD="TEXT"
      NUM_ROW="40"
    >
      <DEFAULT>","</DEFAULT>
    </PARAMETER>
  </PARAMETERS>
  
  <ADVANCED_PARAMETERS>
  	    <PARAMETER
      NAME="DEDUPLICATE"
      FIELD="CHECK"
      NUM_ROW="20"
      SHOW="true"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
    
	<PARAMETER NAME="CSV_OPTION" FIELD="CHECK" REQUIRED="true"
		REPOSITORY_VALUE="CSV_OPTION" NUM_ROW="25">
		<DEFAULT>false</DEFAULT>
	</PARAMETER>

	<PARAMETER NAME="ESCAPE_CHAR" FIELD="CLOSED_LIST" NUM_ROW="25"
		REPOSITORY_VALUE="ESCAPE_CHAR" SHOW_IF="CSV_OPTION == 'true'">
       <ITEMS DEFAULT="ESCAPE_MODE_DOUBLED">
          <ITEM NAME="ESCAPE_MODE_DOUBLED" VALUE="ESCAPE_MODE_DOUBLED" />
          <ITEM NAME="ESCAPE_MODE_BACKSLASH"   VALUE="ESCAPE_MODE_BACKSLASH" />
       </ITEMS>
	</PARAMETER>
	
	<PARAMETER NAME="TEXT_ENCLOSURE" FIELD="TEXT" NUM_ROW="25"
		REPOSITORY_VALUE="TEXT_ENCLOSURE" SHOW_IF="CSV_OPTION == 'true'">
		<DEFAULT>"""</DEFAULT>
	</PARAMETER>
	<PARAMETER
      NAME="DISCARD_TRAILING_EMPTY_STR"
      FIELD="CHECK"
      NUM_ROW="30"
      SHOW="true"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
    <PARAMETER
      NAME="TRIM"
      FIELD="CHECK"
      NUM_ROW="30"
      SHOW="true"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
  </ADVANCED_PARAMETERS>

  <CODEGENERATION>
	<IMPORTS>
		<IMPORT NAME="CSV_Reader" MODULE="javacsv.jar" REQUIRED="true" />
	</IMPORTS>
  </CODEGENERATION>

  <RETURNS>
    <RETURN NAME="NB_LINE" TYPE="id_Integer" AVAILABILITY="AFTER"/>
  </RETURNS>
</COMPONENT>
