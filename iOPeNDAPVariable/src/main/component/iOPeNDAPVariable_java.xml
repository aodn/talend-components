<COMPONENT>
	<HEADER PLATEFORM="ALL" SERIAL="" VERSION="0.1" STATUS="ALPHA"
		COMPATIBILITY="ALL" AUTHOR="jonescc" RELEASE_DATE="20120720"
		STARTABLE="true" VISIBLE="true" DATA_AUTO_PROPAGATE="false"
		SCHEMA_AUTO_PROPAGATE="true">
		<SIGNATURE />
	</HEADER>
	<FAMILIES>
		<FAMILY>Geo/File/Input</FAMILY>
		<FAMILY>NetCDF/OPeNDAP</FAMILY>
	</FAMILIES>
	<DOCUMENTATION>
		<URL />
	</DOCUMENTATION>
	<CONNECTORS>
		<CONNECTOR CTYPE="FLOW" MAX_INPUT="0" />
		<CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1" />
		<CONNECTOR CTYPE="COMPONENT_OK" />
		<CONNECTOR CTYPE="COMPONENT_ERROR" />
		<CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
		<CONNECTOR CTYPE="RUN_IF" />
	</CONNECTORS>
	<PARAMETERS>
		<PARAMETER NAME="URL" FIELD="TEXT" NUM_ROW="1"
			REQUIRED="true">
			<DEFAULT>"http://example.com/thredds/dodsC/in.nc"</DEFAULT>
		</PARAMETER>
	    <PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" REQUIRED="true" NUM_ROW="2">
      		<TABLE READONLY="true">
        		<COLUMN NAME="name" TYPE="id_String" READONLY="true" CUSTOM="true" LENGTH="255"/>
        		<COLUMN NAME="type" TYPE="id_String" READONLY="true" CUSTOM="true" LENGTH="255"/>
        		<COLUMN NAME="dimensions" TYPE="id_String" READONLY="true" CUSTOM="true" LENGTH="255"/>
      		</TABLE>
    	</PARAMETER>
	</PARAMETERS>
	<CODEGENERATION>
		<IMPORTS>
			<IMPORT NAME="netcdf" MODULE="netcdfAll-4.2.jar" REQUIRED="true" />
		</IMPORTS>
	</CODEGENERATION>
	<RETURNS>
		<RETURN NAME="NB_LINE" TYPE="id_Integer" AVAILABILITY="AFTER" />
	</RETURNS>
</COMPONENT>
